{
  "manifest_version": 2,

  "name": "Chrome Footer Extension",
  "description": "This extension shows a footer at the bottom of all chrome tabs",
  "version": "1.0.0",
  "homepage_url": "https://github.com/bbeck/chrome-footer-extension",

  "icons": {
    "128": "icon.png"
  },

  "options_page": "options.html",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["extension.css"],
      "js": ["extension.js"]
    }
  ],

  "permissions": [
    "storage"
  ]
}
